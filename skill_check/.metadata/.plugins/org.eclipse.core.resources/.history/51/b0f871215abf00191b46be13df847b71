package workspace;
import java.io.*;
import java.util.*;
public class Main {
	public static int fact(int a) {
		if(a==1||a==0)return 1;
		return a*fact(a-1);
	}
	public static void main(String[] args) throws Exception {
		Scanner sc = new Scanner(System.in);
		List list = Arrays.asList(sc.next().getBytes());
		System.out.println(list);
	}
}
