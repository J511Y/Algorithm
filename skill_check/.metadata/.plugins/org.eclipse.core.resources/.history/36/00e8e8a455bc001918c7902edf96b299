package workspace;
import java.util.*;
public class test {
	public static boolean solution(int x) {
	      boolean answer = true;
	      int sum = 0;
	      while(x>0) {
	    	  sum+=x%10;
	    	  x/=10;
	      }
	      return answer;
	  }
	public static void main(String [] args) {
		System.out.println(solution(90));
		
	}
}
