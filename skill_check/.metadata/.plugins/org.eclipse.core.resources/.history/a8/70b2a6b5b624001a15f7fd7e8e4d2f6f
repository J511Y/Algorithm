import java.util.*;
class Main {
	static int gcd(int a,int b) {
		if(b == 0) return a;
		return gcd(b, a % b);
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = "3for the first time in forever";
		String[] arr = s.split(" ");
		for(String str : arr) System.out.println(str.toUpperCase());
	}
}