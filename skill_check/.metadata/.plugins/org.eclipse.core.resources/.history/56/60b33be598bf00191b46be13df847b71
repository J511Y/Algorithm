package workspace;
import java.io.*;
import java.util.*;
import java.util.stream.Collectors;
public class Main {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int r=sc.nextInt();
        for(int i=0; i<r; i++) {
        	int h=sc.nextInt();
        	sc.nextInt();
        	int n=sc.nextInt();
        	System.out.printf("%d%02d\n",n%h,n/h+1);        	
        }
    }
}
