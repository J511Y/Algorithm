package workspace;
import java.util.*;
public class baekjoon_1100 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int idx=0;
		int cnt=0;
		for(int i=0; i<8; i++) {
			for(String s:sc.next().split("")) {
				if(idx%2==0 && s.equals("F")) cnt++;
				idx++;
			}
		}
		System.out.println(cnt);
	}
}
