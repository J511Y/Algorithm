public class skill_check1 {
	public static void main(String[] args) {
		Solution s = new Solution();
		int cnt = 0;
		while(true) {
			long pow = (long)Math.pow(3, cnt++);
			if(pow > 500000000) break;
			System.out.println(pow+"/"+cnt);
		}
