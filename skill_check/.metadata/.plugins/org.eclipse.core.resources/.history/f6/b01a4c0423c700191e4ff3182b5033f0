package workspace;
import java.io.*;
import java.util.*;
import java.util.stream.*;
class Main {
	public static void main(String[] a){
		Scanner s=new Scanner(System.in);
		int input = s.nextInt();
		if(input<100)System.out.println(input);
		else if(input==1000)System.out.println(144);
		else {
			int cnt=0;
			for(int i=111; i<=input; i++)if((i/100-i/10)==(i/10-1%10))cnt++;
			System.out.println(99+cnt+"else");
		}
	}
}
