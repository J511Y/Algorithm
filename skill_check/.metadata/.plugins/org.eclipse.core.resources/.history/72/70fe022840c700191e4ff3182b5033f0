package workspace;
import java.io.*;
import java.util.*;
import java.util.stream.*;
class Main {
	public static void main(String[] a){
		Scanner sc=new Scanner(System.in);
//		List<Integer> list= new ArrayList<>();
//		for(int i=0; i<8; i++)list.add((int)Math.pow(3, i));
		int len=sc.nextInt();
		for(int i=0; i<len; i++) {
			for(int j=0; j<len; j++) {
				System.out.print(((i/3)%3==1 && (j/3)%3==1)?' ':'*');
			}
			System.out.println();
		}
	}
}
