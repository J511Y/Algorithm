package workspace;

import java.io.*;
import java.util.*;
public class Main {
	public static int fact(int a) {
		if(a==0)return 0;
		if(a==1)return 1;
		return a*fact(a-1);
	}
	public static void main(String[] args) throws Exception {
		Scanner sc = new Scanner(System.in);
		while(true) {
			System.out.println(fact(sc.nextInt()));			
		}
	}
}
