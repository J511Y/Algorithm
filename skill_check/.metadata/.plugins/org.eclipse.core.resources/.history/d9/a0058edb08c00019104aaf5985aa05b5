package workspace;
import java.io.*;
import java.util.*;
import java.util.stream.Collectors;
public class Main {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int r=sc.nextInt();
        for(int i=0; i<r; i++) {
        	int h=sc.nextInt();
        	sc.nextInt();
        	int n=sc.nextInt();
        	int nmg=n%h;
        	if(nmg==0)System.out.printf("%d%02d\n",h,n/h);
        	else System.out.printf("%d%02d\n",n%h,n/h+1);        	
        }
    }
}
/*

1 4 10 20 35 56  84  120 165
1 3  6  10 15 21  28  36  45
1 2  3  4   5   6   7    8   9*
*/
