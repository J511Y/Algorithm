package workspace;
import java.util.*;
public class test {
	public static int solution(int num) {
		int result = 0;
		long a = num;
		while(a>1) {
			if(result==500)return-1;
			result+=1;
			if(a%2==0)a/=2;
			else a=a*3+1;
		}
	    return result;
	}
	public static void main(String [] args) {
		long n = 118372;
		byte [] bArr= (n+"").getBytes();
		Arrays.parallelSort(bArr);
		for(byte b : bArr)System.out.print((char)b+" ");
		
	}
}
